.loading_box {
    z-index: 99;
    background-color: #ffffff;
    height: 100%;
    position: fixed;
    width: 100%;
    text-align: center;
    padding: 30% 0;
}

.loading_box>view {
    display: inline-block;
}

@-webkit-keyframes loading {
    0% {
        transform: rotate3d(0,0,1,0deg);
    }

    100% {
        transform: rotate3d(0,0,1,360deg);
    }
}

@keyframes loading {
    0% {
        transform: rotate3d(0,0,1,0deg);
    }

    100% {
        transform: rotate3d(0,0,1,360deg);
    }
}

.loading3 {
    width: 20rpx;
    height: 20rpx;
    background: #e03e16;
    border-radius: 50%;
    margin-left: 15rpx;
}

.loading3_1 {
    animation: loading3 1s linear infinite alternate;
}

.loading3_2 {
    animation: loading3 1s linear infinite alternate;
    animation-delay: 0.5s;
}

.loading3_3 {
    animation: loading3 1s linear infinite alternate;
    animation-delay: 1s;
}

@-webkit-keyframes loading3 {
    0% {
        transform: scale(1);
    }

    100% {
        transform: scale(0);
    }
}

@keyframes loading3 {
    0% {
        transform: scale(1);
    }

    100% {
        transform: scale(0);
    }
}

